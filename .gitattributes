verif = "sim"
while verif == "sim":
    print("Seja bem vindo ao caixa eletrônico da FATEC")
    valor = int(input("Qual será o valor do saque?"))
    nota1 = valor // 1 % 10 
    nota5 = valor // 5 % 10
    nota10 = valor // 10 % 10
    nota20 = valor // 20 % 10
    nota50 = valor // 50 % 10
    nota100 = valor // 100 % 10
    print(f"você poderá sacar {nota1} notas de 1, ou {nota5} notas de 5")
    verif = str(input("quer continuar?\n"))
